{<<"build_tools">>,[<<"mix">>]}.
{<<"description">>,
 <<"A mocking libary for the Elixir language.\n\nWe use the Erlang meck library to provide module mocking\nfunctionality for Elixir. It uses macros in Elixir to expose\nthe functionality in a convenient manner for integrating in\nElixir tests.">>}.
{<<"elixir">>,<<"~> 1.0">>}.
{<<"files">>,
 [<<"lib/mock.ex">>,<<"mix.exs">>,<<"README.md">>,<<"LICENSE.md">>]}.
{<<"licenses">>,[<<"MIT">>]}.
{<<"links">>,
 [{<<"Docs">>,<<"https://jjh42.github.io/mock">>},
  {<<"GitHub">>,<<"https://github.com/jjh42/mock">>}]}.
{<<"maintainers">>,
 [<<"Daniel Olshansky (olshansky.daniel@gmail.com)">>,
  <<"Jonathan J Hunt (j@me.net.nz)">>]}.
{<<"name">>,<<"mock">>}.
{<<"requirements">>,
 [[{<<"app">>,<<"meck">>},
   {<<"name">>,<<"meck">>},
   {<<"optional">>,false},
   {<<"requirement">>,<<"~> 0.8.8">>}]]}.
{<<"version">>,<<"0.3.1">>}.
